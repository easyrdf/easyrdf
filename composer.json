    "version": "0.8.0",
    "type": "library",
    "keywords": ["RDF", "Semantic Web", "Turtle", "RDFa"],
    "homepage": "http://www.easyrdf.org/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nicholas Humfrey",
            "email": "njh@aelius.com",
            "homepage": "http://www.aelius.com/njh/",
            "role": "Developer"
        }
    ],
    "support": {
